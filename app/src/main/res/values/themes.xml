<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Apps_customTheme" parent="Theme.AppCompat">
        <item name="actionBarStyle">@style/Apps_ActionBarStyle</item>

    </style>

    <!--WITHOUT INHERITANCE-->
    <style name="customTheme">
        <item name="android:windowTitleBackgroundStyle">@style/WindowTitleBackground</item>
        <item name="android:windowTitleSize">50dip</item>
        <item name="android:windowTitleStyle">@style/windowTitleStyle</item>
          <item name="actionBarStyle">@style/Apps_ActionBarStyle</item>
    </style>
    <style name="sis_theme" parent="android:Theme">
        <item name="android:windowTitleBackgroundStyle">@style/WindowTitleBackground</item>
        <!--<item name="android:windowTitleBackgroundStyle">@style/Sis_WindowTitleBackground</item>-->
        <item name="android:windowTitleSize">50dip</item>
        <item name="android:windowTitleStyle">@style/windowTitleStyle</item>
        <item name="android:icon">@drawable/ic_logo_2_min</item>
        <item name="displayOptions">useLogo|showHome|showTitle</item>
    </style>
    <style name="event_Theme" parent="android:Theme">
        <item name="android:windowTitleBackgroundStyle">@style/WindowTitleBackground</item>
        <!--<item name="android:windowTitleBackgroundStyle">@style/Event_WindowTitleBackground</item>-->
        <item name="android:windowTitleSize">50dip</item>
        <item name="android:windowTitleStyle">@style/windowTitleStyle</item>
        <item name="actionBarStyle">@style/Apps_ActionBarStyle</item>
    </style>
    <style name="Transp_Theme" parent="android:Theme">
        <item name="android:windowTitleBackgroundStyle">@style/WindowTitleBackground</item>
        <!--<item name="android:windowTitleBackgroundStyle">@style/Transp_WindowTitleBackground</item>-->
        <item name="android:windowTitleSize">50dip</item>
        <item name="android:windowTitleStyle">@style/windowTitleStyle</item>
        <item name="actionBarStyle">@style/Apps_ActionBarStyle</item>
    </style>


</resources>